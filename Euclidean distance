#include<stdio.h>
#include<conio.h>

struct euclidian
{
int distance;
}dis,dis1;

void main()
 {
   int sum=0;
   printf("\nEnter the distance between first two points:");
   scanf("%d",&dis.distance);
   printf("\nEnter the distance between another two points:");
   scanf("%d",&dis1.distance);
   sum=dis.distance+dis1.distance;
   printf("\nTotal distance = %d",sum);
   getch();
 }
